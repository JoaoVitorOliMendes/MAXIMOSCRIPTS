 sqlText = 
   "select  " 
 + " asset.assetnum,asset.location, asset.description "
 + "from asset " 
 + "where " +  params["where"] +  " and ( "  + "asset.siteid in (select defsite from maxuser where loginid =  '"+params["userName"]+"' )" + " ) " 
